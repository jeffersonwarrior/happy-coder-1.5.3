{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Happy",
  "version": "1.5.3",
  "identifier": "com.slopus.happy",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8081",
    "beforeDevCommand": "npm start",
    "beforeBuildCommand": "npm run expo:export",
    "distDir": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Happy",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}